    "babel-preset-env": "^1.7.0",
    "core-js": "^2.4.1",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack-bundle-tracker": "^0.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {}
